@model SearchResults

@{
    ViewData["Title"] = "Results";
}
<div class="card">
    <div class="card-header">
        <h3 class="card-title">Packages</h3>
    </div>
    <div class="card-body">
        <div class="">
            @foreach (var package in Model.Packages)
            {
                <partial name="_PackageDisplay" model="package" />
            }
        </div>
    </div>
</div>

<div class="card">
    <div class="card-header">
        <h3 class="card-title">Gits</h3>
    </div>
    <div class="card-body">
        @foreach (var gits in Model.GitRepos)
        {
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">@gits.Key</h4>
                    @foreach (var p in gits.Value)
                    {
                        <partial name="_PackageDisplay" model="p" />
                    }
                </div>
            </div>
        }

    </div>
</div>